<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.project1forcreativeitinstitute.orrio.views.starter.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start" >
        <action
            android:id="@+id/action_startFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_startFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.project1forcreativeitinstitute.orrio.views.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_dasbordFragment"
            app:destination="@id/dasbordFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.project1forcreativeitinstitute.orrio.views.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_dasbordFragment"
            app:destination="@id/dasbordFragment" />
    </fragment>
    <fragment
        android:id="@+id/dasbordFragment"
        android:name="com.project1forcreativeitinstitute.orrio.views.dashbord.DasbordFragment"
        android:label="fragment_dasbord"
        tools:layout="@layout/fragment_dasbord" />
</navigation>